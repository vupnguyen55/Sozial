  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "mysql2": "^1.6.5",
    "sequelize": "^4.42.0",
    "sql": "^0.78.0"
  }
